#count symmetric integers

class Solution:
    def countSymmetricIntegers(self, low: int, high: int) -> int:
        count = 0
        for x in range(low, high + 1):
            s = str(x)
            if len(s) % 2 == 0:
                n = len(s) // 2
                first_sum = sum(int(d) for d in s[:n])
                last_sum = sum(int(d) for d in s[n:])
                if first_sum == last_sum:
                    count += 1
        return count
